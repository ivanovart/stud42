    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.0",
    "yaml-loader": "^0.8.0"
  }
