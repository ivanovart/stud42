{
  "name": "s42-interface",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "generate": "yarn run generate:gql",
    "generate:gql": "graphql-codegen --config graphqlcodegen.yml",
    "dev": "next dev",
    "dev:watch": "concurrently \"yarn run dev\" \"yarn run generate --watch\"",
    "build": "next build",
    "start": "next start",
    "style:all": "yarn run style:lint && yarn run style:prettier&& yarn run style:ts",
    "style:lint": "next lint",
    "style:ts": "yarn run tsc -t es6",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest --watch",
    "test:ci": "jest --passWithNoTests --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@apollo/react-hooks": "^4.0.0",
    "@headlessui/react": "^1.7.7",
    "@sentry/browser": "^7.47.0",
    "@sentry/nextjs": "^7.47.0",
    "@sentry/node": "^7.47.0",
    "@types/uuid": "^9.0.1",
    "apollo3-cache-persist": "^0.14.1",
    "axios": "^1.2.1",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "deepmerge": "^4.2.2",
    "framer-motion": "^10.12.16",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.0",
    "isomorphic-unfetch": "^3.1.0",
    "jose": "^4.14.4",
    "js-cookie": "^3.0.1",
    "next": "^13.4.4",
    "next-auth": "^4.21.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0",
    "twemoji": "^14.0.2",
    "uuid": "^9.0.0",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "concurrently": "^7.6.0",
    "cypress": "^12.1.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
